[Unit]
Description=task que sube servidor para flask de app1-front
After=network.target

[Service]
WorkingDirectory=%h
ExecStart=%h/ejercicio3/subir_app1-front.sh
ExecStop=%h/ejercicio3/bajar_app1-front.sh
Type=simple
KillMode=process
Restart=on-failure
RestartSec=10

[Install]
WantedBy=default.target
